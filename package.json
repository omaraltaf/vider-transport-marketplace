{
  "name": "vider-transport-marketplace",
  "version": "1.0.0",
  "description": "Peer-to-peer marketplace platform for Norwegian B2B transport and logistics",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run dev:rebirth",
    "dev:rebirth": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd server && npx prisma generate && npm run build",
    "build:client": "cd client && npm run build",
    "build:production": "npm run build",
    "start": "cd server && npm start",
    "test": "concurrently \"cd server && npm test\" \"cd client && npm test\"",
    "lint": "concurrently \"cd server && npm run lint\" \"cd client && npm run lint\"",
    "migrate": "cd server && npx prisma migrate dev",
    "migrate:deploy": "cd server && npx prisma migrate deploy",
    "db:studio": "cd server && npx prisma studio",
    "seed": "cd server && npm run seed",
    "deploy:full": "npm run build && npm run migrate:deploy && npm run seed"
  },
  "keywords": [
    "marketplace",
    "transport",
    "logistics",
    "b2b"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@prisma/client": "^5.22.0",
    "@types/nodemailer": "^7.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "openapi-types": "^12.1.3",
    "pdfkit": "^0.17.2",
    "recharts": "^3.5.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/archiver": "^7.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jsdom": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "@types/recharts": "^1.8.29",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "fast-check": "^3.23.2",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "prisma": "^5.7.0",
    "supertest": "^7.1.4",
